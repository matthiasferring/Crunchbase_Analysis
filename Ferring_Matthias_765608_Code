### Main data  frame

## Loading and cleaning data

df <- read.csv("2010_2021_merged_final.csv")
df <- df[,-c(2,5,7,8,15,16)]
df$Money.Raised.Currency..in.USD. <- as.numeric(df$Money.Raised.Currency..in.USD.)
df$Total.Funding.Amount <- as.numeric(df$Total.Funding.Amount)
df

unique(df$Total.Funding.Amount.Currency..in.USD.)

## Funding by round/industry

df <- df %>% 
    group_by(Investor.Names) %>% 
    mutate(Investor.Names=strsplit(Investor.Names, ",")) %>% 
    unnest(Investor.Names)

df <- df %>% 
    group_by(Organization.Industries) %>% 
    mutate(Organization.Industries=strsplit(Organization.Industries, ",")) %>% 
    unnest(Organization.Industries)

df <- df %>%
  add_column(Announced.Year = "")
  df$Announced.Year <- substr(df$Announced.Date, 1, 4)
  
df <- df %>%
  group_by(Organization.Location)%>%
  dplyr::mutate(id=cur_group_id())
colnames(df)[colnames(df) == "id"] <- "location_id"

  
